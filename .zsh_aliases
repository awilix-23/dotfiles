alias ls='ls --color=auto'
alias ll='ls -ltra --color=auto'
alias grep='grep --color=auto'
alias sudo='doas'
alias x='exit'
alias cd='z'
alias ..='cd ..'
alias c='clear'
alias cls='clear && ls'
alias cll='clear && ls -ltra'
alias vi='nvim'
alias vim='nvim'
alias bat='cat /sys/class/power_supply/BAT0/capacity'
alias conn='nmcli connection show'
alias wifi='nmcli device wifi connect'
alias tkill='tmux kill-session -t'
alias stow='stow -t ~'
